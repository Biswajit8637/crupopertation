using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace crudoperation.Entity
{
    public class crudclass
    {
        public string id { get; set; } = null;

        public string fname { get; set; } = null;

        public string lname { get; set; } = null;

        public string rollno { get; set; } = null;

        public string dob { get; set; } = DateTime.Now.ToString();

        public string gender { get; set; } = null;

        public string socialcate { get; set; } = null;

        public string reservcate { get; set; } = null;

        public string disabily { get; set; } = null;

        public double disabiltyprcnt { get; set; } = 0.0;
        public string lodia { get; set; } = null;
    }
}
